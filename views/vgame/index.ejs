<%- include('../partials/header') %>

<h2>Video Games</h2>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
    </tr>
  </thead>
  <tbody>
    <% vgames.forEach(function(v) { %>
    <tr>
      <td><%= v.title %></td>
      <td><%= v.releaseYear %></td>
      <td><%= v.esrbRating %></td>
      <a id="button" href="/vgames/<%= vgame._id %>">Details</a>
    </tr>
    <% }) %>
  </tbody>
</table>

<%- include('../partials/header') %>
