<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Title</th>
      <th>Release<br />Year</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% vgames.forEach(function(v) { %>
    <tr>
      <td><%= v.title %></td>
      <td><%= v.releaseYear %></td>
      <td><a href="/vgames/<%= v._id %>">DETAILS</a></td>
    </tr>
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>
